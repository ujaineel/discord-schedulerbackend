{
  "name": "discord-scheduler-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "local": "NODE_ENV=local concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "prepare": "is-ci || husky install",
    "prisma:generate": "dotenv -e ./env/.env.$NODE_ENV -- npx prisma generate",
    "migrate:prisma:push": "dotenv -e ./env/.env.$NODE_ENV -- npx prisma db push --preview-feature",
    "migrate:prisma": "dotenv -e ./env/.env.$NODE_ENV -- npx prisma migrate dev --preview-feature",
    "migrate:diff": "dotenv -e ./env/.env.$NODE_ENV -- npx prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script",
    "migrate:resolve": "dotenv -e ./env/.env.$NODE_ENV -- npx prisma migrate resolve --applied 000000000000_squashed_migrations",
    "test": "jest",
    "swagger": "ts-node ./src/api-docs/swagger.mjs",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:unit": "jest --testPathPattern=/test/unit --passWithNoTests",
    "test:unit:watch": "jest --watchAll --testPathPattern=/test/unit --passWithNoTests",
    "testenv:integration": "NODE_ENV=testing dotenv -e ./env/.env.testing -- concurrently \"npm run docker:up\" \"npx prisma migrate deploy\" \"jest  --testPathPattern=test/integration --maxWorkers=1 --passWithNoTests\"",
    "test:integration:watch": "jest --watchAll --testPathPattern=/test/integration --passWithNoTests",
    "test:cov:local": "concurrently \"npm run docker:up\" \"npx prisma migrate deploy\" \"jest --coverage --testPathPattern=test/integration --maxWorkers=1 --passWithNoTests\" \"npm run docker:down\"",
    "test:cov": "jest --coverage --maxWorkers=1 --passWithNoTests",
    "update:env:file": "npx dotenv-vault push $env ./env/.env.$nodeEnv"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-vault": "^1.18.0",
    "dotenv-vault-core": "^0.7.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@faker-js/faker": "^7.6.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.42",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-openapi": "^1.9.0",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.5",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "babel-jest": "^29.5.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.0",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.6",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.11.0",
    "typescript": "^5.0.2"
  }
}
