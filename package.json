{
  "name": "discord-scheduler-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "NODE_ENV=local concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "prepare": "is-ci || husky install",
    "migrate:prisma:push": "dotenv -e ./env/.env.local -- npx prisma db push --preview-feature",
    "migrate:prisma:dev": "dotenv -e ./env/.env.local -- npx prisma migrate dev --preview-feature",
    "migrate:diff:dev": "dotenv -e ./env/.env.local -- npx prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script > ./prisma/migrations/000000000000_squashed_migrations/migration.sql",
    "migrate:resolve:dev": "dotenv -e ./env/.env.local -- npx prisma migrate resolve --applied 000000000000_squashed_migrations",
    "test": "jest",
    "swagger": "ts-node ./src/api-docs/swagger.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.1",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.42",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-openapi": "^1.9.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.6",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.11.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
